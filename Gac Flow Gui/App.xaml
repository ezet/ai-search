             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:flow="clr-namespace:eZet.Csp.Flow">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <flow:MefBootstrapper x:Key="bootstrapper" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>